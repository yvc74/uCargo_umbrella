   <div class="shell">
				<section class="assignment_container">
					<div class="container__information-order">
			      <div class="information-order__import">
							<div class="import__import-box">
								<div class=import-box__import>
									<p><%= render_order_type(@order.type) %></p>
								</div>
								<div class="import-box__puja">
									<p class="container__text-gray">PUJA:</p>
								</div>
								<div class="import-box__hours">
									<p class="container__text-blue"><%= render_difference_date(@planning.auction.begin_date, @planning.auction.end_date) %> HORAS </p>
								</div>
							</div>
							<div class="import__place">
								<div class="place__departure">
									<i class="fas fa-angle-up"></i>
									<p><%= render_uppercase(@order.custom.name) %></p>
								</div>
								<div class="place__arrival">
									<i class="fas fa-angle-down"></i>
									<p><%= render_uppercase(@order.delivery.name) %>.</p>
								</div>
							</div>
						</div>
						<div class="container__reference">
							<div class="reference__ref-ucargo-box">
								<div class="ref-ucargo-box__reference">
									<p class="container__text-gray">REF. UCARGO</p>
								</div>
								<div class="ref-ucargo-box__numbers">
									<p class="container__text-blue"><%= @order.planning_id %></p>
								</div>
							</div>
							<div class="reference__ref-house-box">
								<div class="ref-house-box__reference">
									<p class="container__text-gray">REF. HOUSE</p>
								</div>
								<div class="ref-house-box__numbers">
									<p class="container__text-blue"><%= @planning.house_reference %></p>
								</div>
							</div>
							<div class="reference__ref-master-box">
								<div class="ref-master-box__reference">
									<p class="container__text-gray">REF. MASTER</p>
								</div>
								<div class="ref-master-box__numbers">
									<p class="container__text-blue"><%= @planning.master_reference %></p>
								</div>
							</div>
						</div>
						<div class="container__transport">
							<div class="transport__transport-box">
								<div class="transport-box__text">
									<p class="container__text-gray">TRANSPORTE</p>
								</div>
								<div class="transport-box__description">
									<p class="container__text-blue"><%= render_uppercase(@order.transport) %></p>
								</div>
							</div>
							<div class="transport__commodity-box">
								<div class="commodity-box__text">
									<p class="container__text-gray">MERCANCÍA</p>
								</div>
								<div class="commodity-box__description">
									<p class="container__text-blue"><%= render_uppercase(@order.merchandise_type) %></p>
								</div>
							</div>
						</div>
						<div class="container__sender-weight">
							<div class="sender-weight__sender">
								<div class="sender__text">
									<p class="container__text-gray">SENDER</p>
								</div>
								<div class="sender__description">
									<p class="container__text-blue"><%= @custom_broker.company %></p>
								</div>
							</div>
							<div class="sender-weight__weight">
								<div class="weight__text">
									<p class="container__text-gray">PESO</p>
								</div>
								<div class="weight__description">
									<p class="container__text-blue"><%= render_uppercase(@order.weight) %></p>
								</div>
							</div>
						</div>
			      <div class="container__carrier-profile">
							<div class="carrier-profile__photo">
								<img src="#" />
							</div>
							<div class="carrier-profile__profile-data">
								<div class="profile-data__text">
									<p>PERFIL TRANSPORTISTA </p>
								</div>
								<div class="profile-data__user-name">
									<p><%= @driver.username %></p>
									<p><%= @driver.name %></p>
								</div>
								<div class="profile-data__qualification">
								<%= for _ <- 1..@driver.score do %>
										<i class="fas fa-star"></i>
									<% end %>
								</div>
							</div>
			      </div>
			      <div class="container__status">
			        <div class="status__text">
			          <p>STATUS</p>
			        </div>
			        <div class="status_road-to">
			          <div class="container__gray-font">
			            <p>VA CAMINO A ADUANA</p>
			          </div>
			          <div class="container__green-icon">
			            <i class="fas fa-check-circle"></i>
			          </div>
			          <div class="container__black-icons">
			            <i class="fas fa-share-alt"></i>
			          </div>
			        </div>
			        <div class="status__trafficlight">
			          <div class="container__gray-font">
			            <p>SEMAFORO</p>
			          </div>
			          <div class="container__green-icon">
			            <i class="fas fa-check-circle"></i>
			          </div>
			          <div class="container__black-icons">
			            <i class="fas fa-share-alt"></i>
			          </div>
			        </div>
			        <div class="status__padlock-photo">
			          <div class="container__gray-font">
			            <p>FOTO CANDADO</p>
			          </div>
			          <div class="container__green-icon">
			            <i class="fas fa-check-circle"></i>
			          </div>
			          <div class="container__black-icons">
			            <i data-remodal-target="shared" class="fas fa-share-alt"></i>
			            <i data-remodal-target="preview" class="fas fa-eye"></i>
			          </div>
			        </div>
			        <div class="status__travel-start">
			          <div class="container__gray-font">
			            <p>INICIO DE VIAJE</p>
			          </div>
			          <div class="container__green-icon">
			            <i class="fas fa-check-circle"></i>
			          </div>
			          <div class="container__black-icons">
			            <i class="fas fa-share-alt"></i>
			          </div>
			        </div>
			        <div class="status__arrival-destination">
			          <div class="container__gray-font">
			            <p>LLEGADA A DESTINO</p>
			          </div>
			          <div class="container__green-icon">
			            <i class="fas fa-check-circle"></i>
			          </div>
			          <div class="container__black-icons">
			            <i class="fas fa-share-alt"></i>
			          </div>
			        </div>
			        <div class="status__delivered-customer">
								<div class="container__gray-font">
			            <p>ENTREGADO A CLIENTE (FIRMA) </p>
			          </div>
			          <div class="container__green-icon">
			            <i class="fas fa-check-circle"></i>
			          </div>
			          <div class="container__black-icons">
			            <i class="fas fa-share-alt"></i>
									<i class="fas fa-eye"></i>
			          </div>
			        </div>
			        <div class="status__complete-order">
								<p>+ ver pedido completo</p>
			        </div>
			      </div>
			      <div class="container__origin-address">
							<div class="origin-address__text">
								<p>DIRECCIÓN DE ORIGEN</p>
							</div>
							<div class="origin-address__shipping-place">
								<p class="container__text-blue"><%= render_uppercase(@order.custom.address) %></p>
							</div>
							<div class="origin-address__date-responsable">
								<div class="origin-address__date-collection">
									<p class="container__text-gray">FECHA DE RECOLECCION:<span class="container__text-blue"><%= @order.custom.date %></span></p>
								</div>
								<div class="origin-address__responsible-name">
									<p class="container__text-gray">RESPONSABLE:<span class="container__text-blue"><%= render_uppercase(@order.custom.responsible) %></span></p>
								</div>
							</div>
							<div class="origin-address__collection-time">
								<p class="container__text-gray">HORARIO DE RECOLECCION:<span class="container__text-blue"><%= render_uppercase(@order.custom.schedule) %> HORAS</span></p>
							</div>
			      </div>
			      <div class="container__destination-address">
							<div class="destination-address__text">
								<p>DIRECCIÓN DE DESTINO</p>
							</div>
							<div class="destination-address__delivery-address">
								<p class="container__text-blue"><%= render_uppercase(@order.delivery.address) %></p>
							</div>
							<div class="destination-address__date-delivery">
								<p class="container__text-gray">FECHA DE ENTREGA:<span class="container__text-blue"><%= @order.delivery.date %></span></p>
							</div>
							<div class="destination-address__delivery-time">
								<p class="container__text-gray">HORARIO DE ENTREGA:<span class="container__text-blue"><%= render_uppercase(@order.delivery.schedule) %> HORAS</span></p>
							</div>
							<div class="destination-address__responsable">
								<p class="container__text-gray">RESPONSABLE:<span class="container__text-blue"><%= render_uppercase(@order.delivery.responsible) %></span></p>
							</div>
			      </div>
			      <div class="container__price">
							<div class="price__box">
<<<<<<< Updated upstream
								<p>$<span>MXN+IVA</span></p>
=======
								<p>$<%= @bid.price %><span>MXN+IVA</span></p>
>>>>>>> Stashed changes
							</div>
			      </div>
			    </div>
			    <div class="container__map">
						<div class="map__movable">
							<div class="map__tracking-text">
								<p>RASTREO TIEMPO REAL<span>(COMIENZA EN INICIO DE VIAJE)</span></p>
							</div>
							<div class="map__tracking">
								<div id="map"></div>
							</div>
						</div>
			    </div>
				</section>
			</div>
</main>
